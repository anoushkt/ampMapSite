<h1>Enter your choices</h1>
<%= form_for(@book) do |f| %>
  <ul>
  <% @book.errors.full_messages.each do |book| %>
    <li><%= error %></li>
  <% end %>
  </ul>
  <p>
    <%= f.label :af %><br />
    <%= f.text_field :af %>
  </p>
  <p>
    <%= f.label :protocol %><br />
    <%= f.text_area :protocol %>
  </p>

  <p>
    <%= f.label :country %><br />
    <%= f.text_area :country%>
  </p>


  <p>
    <%= f.submit %>
  </p>
<% end %>
